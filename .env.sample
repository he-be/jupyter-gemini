# JupyterHub Configuration
JUPYTERHUB_ADMIN_USERS=admin@example.com,manager@example.com
JUPYTERHUB_COOKIE_SECRET=generate-your-cookie-secret-here
JUPYTERHUB_PROXY_AUTH_TOKEN=generate-your-proxy-token-here

# Authentication Settings
ALLOWED_USERS=user1@example.com,user2@example.com
ALLOWED_EMAIL_DOMAINS=example.com,company.com
OPEN_SIGNUP=false

# Docker Configuration
DOCKER_USER_IMAGE=jupyter-user-gemini:latest
USER_CPU_LIMIT=2.0
USER_MEM_LIMIT=4G

# JupyterHub Logging
JUPYTERHUB_LOG_LEVEL=INFO

# Email Authentication
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-email@gmail.com
SMTP_PASSWORD=your-app-password

# Cloudflare Configuration (External Tunnel)
# Note: Cloudflare tunnel is managed by external server at 192.168.0.200
# JupyterHub will be accessible at your-domain.com via existing tunnel
CLOUDFLARE_DOMAIN=your-domain.com
JUPYTER_SUBDOMAIN=jupyter
# External access: https://jupyter.your-domain.com -> 192.168.0.202:8000

# Gemini CLI
GEMINI_API_KEY=your-gemini-api-key

# Docker Configuration
DOCKER_NETWORK=jupyterhub_network

# Security
JWT_SECRET=generate-your-jwt-secret-here